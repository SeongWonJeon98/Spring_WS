package Basic;

public class OrderManager {
	private HyundaiMaker hyundaiMaker;

	public OrderManager() {
		super();
	}

	public OrderManager(HyundaiMaker hyundaiMaker) {
		super();
		this.hyundaiMaker = hyundaiMaker;
	}
	
	public void Order() {
		Money money = new Money(10000);
		Car car = HyundaiMaker("판매상(인수) : " + car.getName());
	}
	
}
